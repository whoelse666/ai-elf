{
  "name": "ai-elf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite -m alpha",
    "build:alpha": "vite build -m alpha",
    "build:beta": "vite build -m beta",
    "build:stable": "vite build -m stable",
    "preview:alpha": "pnpm build:alpha && vite preview -m alpha",
    "preview:beta": "pnpm build:beta && vite preview -m beta",
    "preview:stable": "pnpm build:stable && vite preview -m stable",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "build:check": "run-p type-check build-only",
    "build-only": "vite build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "pinia": "^2.1.6",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  }
}
